{
    "name": "sailingbyte/laravel-full-calendar",
    "description": "Laravel helper for FullCalendar.io. Fork of https://github.com/nelkasovic/laravel-full-calendar",
    "authors": [
        {
            "name": "Sailingbyte",
            "email": "developers@sailingbyte.com",
            "homepage": "https://sailingbyte.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "illuminate/database": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "laravel/helpers": "^1.5"
    },
    "license": "MIT",
    "homepage": "https://github.com/sailingbyte/laravel-full-calendar",
    "support": {
        "email": "developers@sailingbyte.com",
        "issues": "https://github.com/sailingbyte/laravel-full-calendar/issues",
        "wiki": "https://github.com/sailingbyte/laravel-full-calendar/wiki",
        "source": "https://github.com/sailingbyte/laravel-full-calendar",
        "docs": "https://github.com/sailingbyte/laravel-full-calendar"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "LaravelFullCalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFullCalendar\\FullCalendarServiceProvider"
            ],
            "aliases": {
                "Calendar": "LaravelFullCalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
